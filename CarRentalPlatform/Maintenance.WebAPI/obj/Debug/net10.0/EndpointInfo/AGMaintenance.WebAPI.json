{
  "openapi": "3.0.1",
  "info": {
    "title": "AGMaintenance.WebAPI",
    "version": "v1"
  },
  "paths": {
    "/api/maintenance/vehicles/{vehicleId}/repairs": {
      "get": {
        "tags": [
          "AGMaintenance"
        ],
        "parameters": [
          {
            "name": "vehicleId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/maintenance/repairs": {
      "post": {
        "tags": [
          "AGMaintenance"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AGRepairHistoryDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AGRepairHistoryDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AGRepairHistoryDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/maintenance/crash": {
      "get": {
        "tags": [
          "AGMaintenance"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/maintenance/usage": {
      "get": {
        "tags": [
          "AGMaintenance"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AGRepairHistoryDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "vehicleId": {
            "type": "integer",
            "format": "int32"
          },
          "repairDate": {
            "type": "string",
            "format": "date-time"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "cost": {
            "type": "number",
            "format": "double"
          },
          "performedBy": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "ApiKey": {
        "type": "apiKey",
        "description": "Enter API Key",
        "name": "X-Api-Key",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "ApiKey": [ ]
    }
  ]
}